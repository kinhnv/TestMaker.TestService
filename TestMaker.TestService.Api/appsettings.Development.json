{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ACS": [
    {
      "Type": "JsonFile",
      "Optional": true,
      "Path": "appsettings.Development.Extensions.json"
    }
  ],
  "Server": {
    "IdentityServer": "http://localhost:60001"
  }
}
